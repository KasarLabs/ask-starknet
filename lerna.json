{
  "version": "independent",
  "npmClient": "pnpm",
  "command": {
    "publish": {
      "ignoreChanges": [
        "*.md",
        "*.test.ts",
        "*.spec.ts"
      ],
      "message": "chore(release): publish %s",
      "gitTagVersion": false,
      "push": false
    },
    "version": {
      "allowBranch": "*",
      "message": "chore(release): version packages",
      "gitTagVersion": false,
      "push": false
    }
  }
}
