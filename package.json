{
  "name": "snaknet-plugins",
  "version": "0.0.1",
  "description": "Monorepo of Starknet plugins for testing and development",
  "author": "Kasar Labs",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@9.3.0",
  "keywords": [
    "starknet",
    "blockchain",
    "plugins",
    "monorepo"
  ],
  "scripts": {
    "clean": "lerna run clean",
    "clean:all": "lerna run clean:all",
    "clean:dist": "lerna run clean:dist",
    "check-types": "turbo run check-types",
    "build": "turbo build",
    "build:plugins": "turbo build --scope \"@snakagent/plugin-*\"",
    "test": "turbo test",
    "test:plugins": "turbo test --scope \"@snakagent/plugin-*\"",
    "lint": "lerna run lint --parallel",
    "lint:fix": "lerna run lint:fix --parallel",
    "lint:plugins": "lerna run lint --scope \"@snakagent/plugin-*\"",
    "format": "prettier --write \"plugins/**/*.{ts,tsx,js,jsx,json,md}\"",
    "docker-setup": "docker compose -f ./docker-compose.dev.yml up -d",
    "docker-down": "docker compose -f ./docker-compose.dev.yml down"
  },
  "dependencies": {
    "starknet": "^6.23.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "lerna": "^8.2.0",
    "prettier": "^3.6.1",
    "ts-jest": "^29.2.6",
    "turbo": "^2.4.4",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}