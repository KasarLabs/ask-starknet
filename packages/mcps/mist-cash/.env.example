# Starknet Account Configuration
STARKNET_PRIVATE_KEY=your_private_key_here
STARKNET_ACCOUNT_ADDRESS=your_account_address_here
STARKNET_RPC_URL=https://starknet-sepolia.public.blastapi.io/rpc/v0_7

# Test Token Symbol (defaults to ETH if not set)
# Supported symbols: ETH, STRK, USDC, USDT, WBTC, SWSS
TEST_TOKEN_SYMBOL=ETH

# Recipient address for deposits/withdrawals (required for tests)
TEST_RECIPIENT_ADDRESS=

# For withdrawal tests - set these after running a successful deposit
TEST_CLAIMING_KEY=
TEST_AMOUNT=

# For testing already withdrawn transactions
TEST_WITHDRAWN_CLAIMING_KEY=
TEST_WITHDRAWN_AMOUNT=
